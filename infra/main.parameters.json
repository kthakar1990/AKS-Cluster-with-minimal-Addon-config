{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "clusterName": {
      "value": "aks-minimal-demo"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "environment": {
      "value": "dev"
    },
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "kubernetesVersion": {
      "value": ""
    },
    "nodeVmSize": {
      "value": "Standard_D2s_v3"
    },
    "nodeCount": {
      "value": 1
    },
    "enableWorkloadIdentity": {
      "value": false
    },
    "enableMonitoring": {
      "value": false
    },
    "osDiskType": {
      "value": "Ephemeral"
    },
    "osDiskSizeGB": {
      "value": 30
    }
  }
}
